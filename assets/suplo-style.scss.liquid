	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	background-color: #fff;
}
#header .header-desktop .desktop-cart-wrapper .quickview-cart ul::-webkit-scrollbar{
	width: 6px;
	background-color: #fff;
}
#header .header-desktop .desktop-cart-wrapper .quickview-cart ul::-webkit-scrollbar-thumb{
	background-color: $colorPrimary;
}
#header .mobile-header .desktop-cart-wrapper1 .quickview-cart ul::-webkit-scrollbar-track{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	background-color: #fff;
}
#header .mobile-header .desktop-cart-wrapper1 .quickview-cart ul::-webkit-scrollbar{
	width: 6px;
	background-color: #fff;
}
#header .mobile-header .desktop-cart-wrapper1 .quickview-cart ul::-webkit-scrollbar-thumb{
	background-color: $colorPrimary;
}
#header {
	&.scrolldown {
		position: fixed;
		top: 0;
		left: 0;
		right: 0;
		width: 100%;
		z-index: 999;
	}
	.header-desktop {
		.header-top {
			padding: 10px 0px;
			background: $colorBgHeaderTop;
			.header-top-contact {
				span {
					display: inline-block;
					margin: 0px 10px;
					@extend %text-12;
					color: $colorTextHeaderTop;
				}
				a {
					display: inline-block;
					@extend %text-12;
					@extend %smooth-trans;
					@extend %text-bold;
					color: $colorTextHeaderTop;
					&:hover {
						color: $colorTextHeaderTopHover;
					}
				}
			}
			.hdt-right > span {
				display: inline-block;
				margin: 0px 10px;
				@extend %text-12;
				color: $colorTextHeaderTop;
			}
			.header-top-cart, .header-top-account, .header-top-link {
				display: inline-block;
				> span {
					display: inline-block;
					margin: 0px 10px;
					@extend %text-12;
					color: $colorTextHeaderTop;
				}
				> a {
					display: inline-block;
					@extend %text-12;
					@extend %smooth-trans;
					@extend %text-bold;
					color: $colorTextHeaderTop;
					&:hover {
						color: $colorTextHeaderTopHover;
					}
				}
			}
			.desktop-cart-wrapper {
				position: relative;
				.quickview-cart {
					webkit-transition: all 0.3s ease;
					-moz-transition: all 0.3s ease;
					transition: all 0.3s ease;
					display: none;
					text-align: left;
					position: absolute;
					z-index: 9999;
					top: 100%;
					right: 0px;
					width: 280px;
					padding: 5px;
					border-radius: 5px;
					background: #fff;
					line-height: normal !important;
					-webkit-box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.2);
					-moz-box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.2);
					box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.2);
					h3 {
						border-radius: 5px;
						padding: 5px;
						width: 100%;
						display: block;
						background: $colorPrimary;
						color: #fff;
						@extend %text-12;
						@extend %text-bold;
						@extend %text-uppercase;
						span {
							position: absolute;
							color: #fff;
							top: -10px;
							right: -10px;
							cursor: pointer;
							width: 30px;
							height: 30px;
							line-height: 30px;
							background: $colorSecondary;
							text-align: center;
							border-radius: 50%;
						}
					}
					ul {
						margin: 0px;
						max-height: 250px;
						overflow-y: scroll;
						.cart-item {
							margin: 0px 0px 5px 0px;
							position: relative;
							border-bottom: 1px dashed $colorBorder;
							padding-bottom: 5px;
							> .cart__remove {
								position: absolute;
								top: calc(50% - 9px);
								right: 5px;
								color: $colorPrimary;
								@extend %smooth-trans;
								&:hover {
									color: $colorSecondary;
								}
							}
							&:last-child {
								margin: 0px;
							}
						}
						.cart-item-info {
							padding-right: 15px;
							> a {
								color: $colorPrimary;
								@extend %text-12;
								@extend %text-bold;
								webkit-transition: all 0.3s ease;
								-moz-transition: all 0.3s ease;
								transition: all 0.3s ease;
								margin-bottom: 3px;
								display: inline-block;
								&:hover {
									color: $colorSecondary;
								}
							}
							small {
								display: block;
								@extend %text-12;
								margin-bottom: 3px;
								display: inline-block;
							}
						}
						.cart-item-price-quantity {
							padding-right: 15px;
							span {
								display: block;
								@extend %text-12;
								color: $colorTextBody;
								margin-bottom: 3px;
								b {
									color: #d30000;
								}
							}
						}
						.cart-item-img {
							overflow: hidden;
							img {
								vertical-align: middle;
							}
						}
					}
					.qv-cart-total {
						margin: 5px 0px;
						padding: 5px;
						width: 100%;
						display: block;
						background: #f9f9f9;
						color: $colorTextBody;
						@extend %text-14;
						span {
							color: #d30000;
							@extend %text-bold;
						}
					}
					.quickview-cartactions {
						a {
							display: inline-block;
							width: 48%;
							border-radius: 5px;
							text-align: center;
							@extend %text-12;
							@extend %text-bold;
							@extend %text-uppercase;
							@extend %smooth-trans;
							padding: 5px 10px;
							&:first-child {
								float: left;
								color: #fff;
								background: $colorPrimary;
							}
							&:last-child {
								float: right;
								color: #fff;
								color: #fff;
								background: $colorPrimary;
							}
							&:hover {
								background: $colorSecondary;
							}
						}
					}
				}
			}
		}
		.header-center {
			padding: 10px 0px;
			background: $colorBgHeader;
			.header-logo {
				> a {
					display: inline-block;
				}	
