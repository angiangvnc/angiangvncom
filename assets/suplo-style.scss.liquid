						@extend %text-12;
						.cart__remove {
							display: inline-block;
							margin-left: 15px;
							@extend %text-12;
							color: $colorPrimary;
							@extend %smooth-trans;
							&:hover {
								color: $colorSecondary;
							}
						}
					}
				}
			}
		}
	}
	.modal-checkout-actions {
		padding: 15px;
		.text-left {
