// ---------- MODAL ADD COMPLETE ---------- //
.modal-tbl-cart::-webkit-scrollbar-track{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	background-color: #fff;
}
.modal-tbl-cart::-webkit-scrollbar{
	width: 6px;
	background-color: #fff;
}
.modal-tbl-cart::-webkit-scrollbar-thumb{
	background-color: $colorPrimary;
}
#modalAddComplete {
	display: none;
	position: fixed;
	z-index: 9999999; 
	padding-top: 5%;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	overflow: auto;
	background-color: rgb(0,0,0);
	background-color: rgba(0,0,0,0.6);

	.modalAddComplete-content {
		border-radius: 5px;
		position: relative;
		background-color: #fff;
		margin: auto;
		padding: 0;
		border: 1px solid $colorBorder;
		width: 85%;
		max-width: 850px;
		box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2),0 6px 20px 0 rgba(0,0,0,0.19);
		-webkit-animation-name: animatetop;
		-webkit-animation-duration: 0.4s;
		animation-name: animatetop;
		animation-duration: 0.4s;
		#modalAddComplete-close {
			width: 30px;
			height: 30px;
			line-height: 30px;
			border-radius: 50%;
			display: inline-block;
			text-align: center;
			padding: 0px;
			margin: 0px;
			background: $colorSecondary;
			color: #fff;
			font-size: 18px;
			position: absolute;
			top: -15px;
			right: -15px;
			z-index: 99;
			cursor: pointer;
		}
	}

	/* Add Animation */
	@-webkit-keyframes animatetop {
		from {top:-300px; opacity:0} 
		to {top:0; opacity:1}
	}

	@keyframes animatetop {
		from {top:-300px; opacity:0}
		to {top:0; opacity:1}
	}
	.modal-cart-status {
		border-bottom: 1px solid $colorBorder;
		h2 {
			color: $colorPrimary;
			@extend %text-16;
			font-weight: 900;
			@extend %text-uppercase;
			margin: 0px;
			padding: 15px;
			display: block;
		}
	}
	.modal-tbl-cart {
		max-height: 250px;
		overflow-y: scroll;

		table,
		td,
		th {
			border-bottom: 1px solid $colorBorder;
			border-collapse: collapse;
		}

		table {
			th {
				padding: 5px !important;
				color: $colorTextBody;
				min-width: 100px;
			}

			td {
				padding: 5px !important;

				&.product-img {
					text-align: center;

					a {
						img {
							max-width: 100px;
							vertical-align: middle;
						}
					}
				}

				&.product-title {
					> a {
						color: $colorPrimary;
						@extend %text-14;
						@extend %text-bold;
						display: block;
						-webkit-transition: all 0.3s ease;
						-moz-transition: all 0.3s ease;
						transition: all 0.3s ease;

						&:hover {
							color: $colorSecondary;
						}
					}
					small, p {
						display: block;
						@extend %text-12;
						margin: 0px;
					}
				}

				&.product-sku {
					color: $colorTextBody;
				}

				&.product-price {
					position: relative;

					span {
						display: block;

						&.current-price {
							color: #d30000;
							@extend %text-bold;
						}

						&.original-price {
							@extend %text-12;
							color: lighten($colorTextBody,20%);
						}

						&.sale-off {
							display: inline-block !important;
							text-align: center;
							padding: 2px 3px;
							color: #fff !important;
							background: $colorPrimary;
							font-size: 14px !important;
						}
					}
				}

				&.product-money {
					color: #d30000;
					@extend %text-bold;
				}

				&.product-remove {
					> a {
						color: $colorPrimary;
						display: block;
						@extend %smooth-trans;
						&:hover {
							color: $colorSecondary;
						}
					}
				}
			}
		}

	}
