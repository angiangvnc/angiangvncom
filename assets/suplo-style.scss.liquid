#main-slider {
	margin-bottom: 30px;
	#owl-home-main-slider {
		.owl-nav {
			margin: 0;
			> div {
				position: absolute;
				top: calc(50% - 20px);
				height: 40px;
				width: 40px;
				line-height: 40px;
				color: #fff;
				background: $colorPrimary;
				z-index: 9;
				display: inline-block;
				padding: 0px;
				margin: 0px;
				border-radius: 3px;
				@extend %text-24;
				@extend %smooth-trans;
				&.owl-prev {
					left: 15px;
				}
				&.owl-next {
					right: 15px;
				}
				&:hover {
					background: $colorSecondary;
				}
			}
		}
	}
	.ms-item {
		position: relative;
		img {
			width: 100%;
		}
		.slide-content {
			position: absolute;
			top: 50%;
			left: 50%;
			-webkit-transform: translate(-50%, -50%);
			transform: translate(-50%, -50%);
			width: 100%;
			z-index: 1;
			color: #fff;
			line-height: normal;
			.slide-bold-text {
				position: relative;
				font-size: 48px;
				@extend %text-bold;
				@extend %text-uppercase;
				margin-bottom: 50px;
				color: $colorTextMainSlider;
				font-weight: 900;
				&:after {
					content: '';
					position: absolute;
					left: 0;
					right: 0;
					bottom: -20px;
					width: 100px;
					height: 5px;
					background: $colorTextMainSlider;
					margin: auto;
				}
			}
			.slide-light-text {
				@extend %text-18;
				margin-bottom: 30px;
				color: $colorTextMainSlider;
			}
			.slide-btn {
				> a {
					border-radius: 3px;
					display: inline-block;
					padding: 10px 20px;
					@extend %text-18;
					@extend %text-bold;
					@extend %text-uppercase;
					@extend %smooth-trans;
					background: $colorSecondary;
					color: #fff;
					box-shadow: 3px 3px 0px 0px rgba($colorPrimary, 0.87);
				}
			}
		}
	}
}
