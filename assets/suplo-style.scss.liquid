// ---------- POP UP SUBSCRIBE ---------- //
.popup {
	display: none;
	/* Hidden by default */
	position: fixed;
	/* Stay in place */
	z-index: 999999999;
	/* Sit on top */
	left: 0;
	top: 0;
	width: 100%;
	/* Full width */
	height: 100%;
	/* Full height */
	overflow: auto;
	/* Enable scroll if needed */
	background-color: rgb(0,0,0);
	/* Fallback color */
	background-color: rgba(0,0,0,0.8);
	/* Black w/ opacity */

	.popup-content {
		background: url('{{'popup_bg.png' | asset_url}}');
		background-position: center;
		background-repeat: no-repeat;
		background-size: cover;
		margin: auto;
		max-width: 750px;
		width: 90%;
		top: 50px;
		position: relative;

		.popup-wrapper {
			background: rgba(255, 255, 255, 0.8);
			padding: 30px 15px;
		}

		.popup-title {
			@extend %text-18;
			@extend %text-bold;
			color: $colorPrimary;
			margin-bottom: 15px;
		}

		.popup-desc {
			color: $colorTextBody;
			margin-bottom: 15px;
			text-align: justify;
		}

		.popup-list {
			margin-bottom: 15px;

			ul {
				margin: 0;

				li {
					position: relative;
					margin-bottom: 5px;
					padding-left: 15px;

					&:last-child {
						margin: 0 !important;
					}

					&:after {
						content: '\f111';
						font-family: FontAwesome;
						position: absolute;
						top: calc(50% - 3px);
						left: 0;
						font-size: 6px;
						color: $colorPrimary;
						line-height: normal !important;
					}
					color: $colorTextBody;
				}
			}
		}

		.form-desc {
			@extend %text-16;
			color: $colorTextBody;
			margin-bottom: 15px;
		}

		form {
			position: relative;
			width: 100%;

			.input-group {
				width: 100%;
			}

			input {
				width: 100%;
				height: 40px;
				line-height: 40px;
				padding: 0px 45px 0px 15px;
				@extend %text-14;
				color: $colorTextBody;
				background: #fff;
				border: 1px solid $colorPrimary;
				border-radius: 100px !important;
				outline: 0 !important;
				margin-bottom: 15px;
			}

			button {
				position: absolute;
				top: 3px;
				right: 3px;
				height: 34px;
				width: 34px;
				border-radius: 50%;
				display: inline-block;
				background: $colorPrimary;
				color: #fff;
				@extend %text-18;
				@extend %text-bold;
				@extend %text-uppercase;
				@extend %smooth-trans;
				&:hover {
					background: $colorSecondary;
				}
			}
		}

		.popup-social-network {
			display: inline-block;
			@extend %text-18;
			@extend %smooth-trans;
			color: #fff;
			background: $colorPrimary;
			margin-right: 5px;
			margin-bottom: 5px;
			height: 30px;
			width: 30px;
			text-align: center;
			line-height: 30px;
			padding: 0;
			border-radius: 50%;
			&:hover {
				background: $colorSecondary;
			}
		}
	}

	.close-popup {
		position: absolute;
		top: -15px;
		right: -15px;
		color: #fff;
		font-size: 18px;
		font-weight: bold;
		display: block;
		height: 30px;
		width: 30px;
		background-color: $colorSecondary;
		text-align: center;
		z-index: 9999;
		border-radius: 50%;
		line-height: 30px;
	}

	.close-popup:focus,
	.close-popup:hover {
		color: #fff;
		text-decoration: none;
		cursor: pointer;
	}
}
#popup-btn {
	display: none;
}
