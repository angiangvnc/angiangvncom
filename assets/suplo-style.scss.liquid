/* ------- Back to top ------------ */
#back-to-top {
	position: fixed;
	right: 50px;
	line-height: 50px;
	bottom: -150px;
	overflow: hidden;
	z-index: 999;
	@extend %smooth-trans2;
	text-align: center;
	span{
		display: inline-block;
		width: 115px;
		position: relative;
		img{
			max-width: 100%;
			&.hover{
				position: absolute;
				opacity: 0;
				top: 5px;
				left: 21px;
				@extend %smooth-trans;
			}
		}
	}
	&:hover{
		.hover{
			opacity: 1;
		}
	}
	&.car-up{
		bottom: 50px;
	}
	&.car-run{
		animation: car-run 1s;
	}
}
@keyframes car-up{
	0%{
		bottom: -150px;
	}
	100%{
		bottom: 50px;
	}
}
@keyframes car-down{
	0%{
		bottom: 50px;
	}
	100%{
		bottom: -150px;
	}
}
@keyframes car-run{
	0%{
		bottom: 50px;
	}
	100%{
		bottom: 100%;
	}
}
