/* ------------ declaring color variables ------------ */
$colorBorder: {{ settings.color_borders }} /*#e6e6e6*/;
$colorPrimary: {{ settings.color_primary }} /*#000*/;
$colorSecondary: {{ settings.color_secondary }} /*#fff*/;

$colorTextBody: {{ settings.color_body_text }} /*#000*/;
$colorBody: {{ settings.color_body_bg }} /*#fff*/;

$colorTextFooter: {{settings.footer_text_color}} /*#000*/;
$colorTextFooterHover: {{settings.footer_text_color_hover}} /*#000*/;
$colorTextFooterTitle: {{settings.footer_text_color_title}} /*#000*/;
$colorBgFooter: {{settings.footer_bg_color}} /*#fff*/;

$colorTextCopyrights: {{settings.cr_text_color}} /*#fff*/;
$colorTextCopyrightsHover: {{settings.cr_text_color_hover}} /*#fff*/;
$colorBgCopyrights: {{settings.cr_bg_color}} /*#000*/;

$colorTextHeaderTop: {{settings.hdt_text_color}} /*#002e5b*/;
$colorTextHeaderTopHover: {{settings.hdt_text_color_hover}} /*#11b5e6*/;
$colorBgHeaderTop: {{settings.hdt_bg_color}} /*#fff*/;

$colorTextHeader: {{settings.hdc_text_color}} /*#002e5b*/;
$colorTextHeaderHover: {{settings.hdc_text_color_hover}} /*#11b5e6*/;
$colorBgHeader: {{settings.hdc_bg_color}} /*#fff*/;

$colorTextDropdown: {{settings.color_text_dropdown}} /*#000*/;
$colorTextDropdownHover: {{settings.color_text_dropdown_hover}} /*#000*/;
$colorBgDropdown: {{settings.color_bg_dropdown}} /*#fff*/;

$colorTextNavbar: {{settings.navbar_text_color}};
$colorTextNavbarHover: {{settings.navbar_texthover_color}};
$colorBgNavbar: {{settings.nav_bg_color}};

$colorTextMainSlider: {{settings.ms_text_color}};

$colorTextSearch: {{settings.search_text_color}};
$colorBgSearch: {{settings.search_bg_color}};

$colorTextHBanners: {{settings.hbanners_text_color}};
$colorBgHBanners: {{settings.hbanners_bg_color}};

$colorTextHBrands: {{settings.hbrands_text_color}};
$colorBgHBrands: {{settings.hbrands_bg_color}};

/* ------------ declaring inheritance ------------ */

%text-12 {
	font-size: 12px;
}

%text-14 {
	font-size: 14px;
}

%text-16 {
	font-size: 16px;
}

%text-18 {
	font-size: 18px;
}

%text-20 {
	font-size: 20px;
}

%text-24 {
	font-size: 24px;
}

%text-bold {
	font-weight: bold;
}

%text-justify {
	text-align: justify !important;
}

%text-uppercase {
	text-transform: uppercase !important;
}

%smooth-trans {
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
	transition: all 0.3s ease;
}

%smooth-trans1 {
	-webkit-transition: all 0.6s ease;
	-moz-transition: all 0.6s ease;
	transition: all 0.6s ease;
}

%smooth-trans2 {
	-webkit-transition: all 1s ease;
	-moz-transition: all 1s ease;
	transition: all 1s ease;
}

/* ------------ normalize elements / classes ------------ */

input, button, textarea, select {
	outline: 0px;
}

.mobile-nav a:focus {
	background: transparent;
}

body {
	margin: 0;
}

body {
	position: relative;
	line-height: 1.6em;
	overflow-x: hidden;

	button,
	h1,
	h2,
	h3 {
		//line-height: normal;
		font-weight: normal;
	}
}

img {
	vertical-align: middle;
}

.mg-bottom-10 {
	margin-bottom: 10px !important;
}

.mg-bottom-30 {
	margin-bottom: 30px !important;
}

.mg-bottom-50 {
	margin-bottom: 50px !important;
}

.mg-left-0 {
	margin-left: 0 !important;
}

.mg-left-5 {
	margin-left: -5px !important;
}

.mg-left-10 {
	margin-left: -10px !important;
}

.mg-left-15 {
	margin-left: -15px !important;
}

.pd-left0 {
	padding-left: 0 !important;
}

.pd-left5 {
	padding-left: 5px !important;
}

.pd-left10 {
	padding-left: 10px !important;
}

.pd-left15 {
	padding-left: 15px !important;
}

@media (max-width: 768px) {
	body .md-mg-left-5 {
		margin-left: -5px !important;
	}

	body .md-pd-left5 {
		padding-left: 5px !important;
	}

	body .md-mg-left-10 {
		margin-left: -10px !important;
	}

	body .md-pd-left10 {
		padding-left: 10px !important;
	}

	body .md-mg-left-15 {
		margin-left: -15px !important;
	}

	body .md-pd-left15 {
		padding-left: 15px !important;
	}
}

input[type=number] {
	-moz-appearance:textfield;
}

input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
	-webkit-appearance: none;
	margin: 0;
}

.fb_iframe_widget iframe {
	max-width: 1000px;
	z-index: 9;
}

.fb-comments,
.fb-comments > span,
.fb-comments span iframe {
	width: 100% !important;
}

.sidebar-sort {
	.no-bullets {
		li {
			input[type="checkbox"] {
				-webkit-appearance: checkbox;
				-moz-appearance: checkbox;
				-webkit-appearance: none;
				-moz-appearance: none;
				appearance: none;
				width: 16px;
				height: 16px;
				background: none;
				border: 1px solid #c3c3c3;
				display: inline-block;
				overflow:hidden;
				vertical-align: middle;
				&:focus{
					outline: none;
				}
				&:checked {
					width: 16px;
					height: 16px;
					&:after{
						content:'\2714';
						font-size:14px;
						line-height:16px;
						text-align:center;
						display:block;
						outline:none;
						color: $colorPrimary;
						outline: 1px solid #fff;
					}
				}
			}
			input[type="radio"] {
				-webkit-appearance: checkbox;
				-moz-appearance: checkbox;
				-webkit-appearance: none;
				-moz-appearance: none;
				appearance: none;
				width: 16px;
				height: 16px;
				background: none;
				border: 1px solid #c3c3c3;
				display: inline-block;
				overflow:hidden;
				vertical-align: middle;
				&:focus{
					outline: none;
				}
				&:checked {
					width: 16px;
					height: 16px;
					&:after{
						content:'\2714';
						font-size:14px;
						line-height:16px;
						text-align:center;
						display:block;
						outline:none;
						color: $colorPrimary;
						outline: 1px solid #fff;
					}
				}
			}
		}
	}
}

.pagination-custom {
	> span {
		display: inline-block;
		height: 40px;
		width: 40px;
		line-height: 40px;
		margin: 0px 5px;
		border-radius: 5px;
		&.current {
			padding: 0px !important;
			@extend %text-18;
			@extend %text-bold;
			color: #fff;
			background: $colorSecondary;
			border: 1px solid $colorSecondary;
		}
		> a {
			border-radius: 5px;
			padding: 0px;
			display: inline-block;
			text-align: center;
			height: 40px;
			line-height: 40px;
			width: 40px;
			padding: 0px;
			@extend %text-18;
			@extend %text-bold;
			@extend %smooth-trans;
			color: #fff;
			border: 1px solid $colorPrimary;
			background: $colorPrimary;
			&:hover {
				background: $colorSecondary;
				border: 1px solid $colorSecondary;
			}
		}
	}
}

#pagination-page {
	clear: left;
	margin: 0px;
}

.pagination {
	clear: left;
	margin: 0px;
}

.float-right {
	float: right;
}

.swatch .header {
	@extend %text-bold;
	min-width: auto !important;
	margin: 0px 15px 0px 0px;
	float: left;
	line-height: 35px;
}

.select-swap {
	display: inline-block;
}

.js-qty {
	margin: 0px;
	max-width: 150px;
	input, button {
		outline: 0px;
	}
}

.qty-addcart > span {
	margin: 0;
	@extend %text-bold;
	margin-bottom: 5px;
	display: block;
	min-width: 200px;
	position: relative;
}

.swatch-element label span {
	color: $colorTextBody !important;
}
.search-h1 {
	@extend %text-18;
	@extend %text-bold;
	@extend %text-uppercase;
}
/* ------------ breakpoints ------------ */
@media (max-width: 480px) {
	body {
		padding-bottom: 67px;
	}
}
@media (max-width:768px) {
	#page-wrapper{
		.cart{
			.cart-item{
				margin-bottom:15px;
				border-bottom:1px solid $colorBorder;
				.h4{
					font-size:14px !important;
				}
				.price{
					display:inline-block;
					.h3{
						font-size:16px !important;
						color: $colorPrimary;
					}
				}
				.js-qty{
					margin-top: 10px;
					margin-bottom: 10px !important;
				}
				.cart__remove{
					display:inline-block;
					margin-left:10px;
				}
			}
			.btnCart {
				font-size: 14px;
			}
		}
	}
}
// ---------- NAV DRAWER ---------- //
#NavDrawer::-webkit-scrollbar {
	width: 7px;
}
#NavDrawer::-webkit-scrollbar-track {
	-webkit-box-shadow: inset 0 0 5px #fff; 
	border-radius: 0px;
}
#NavDrawer::-webkit-scrollbar-thumb {
	border-radius: 0px;
	-webkit-box-shadow: inset 0 0 5px #fff; 
	background: #fff;
}
#NavDrawer {
	z-index: 999999;
	background: rgba($colorBgNavbar,0.9);
	padding: 0px;
	border-left: none;

	.mobile-nav > .mobile-nav__item {
		background: transparent;
	}

	.drawer__header {
		height: auto;
		border-bottom: 0px;
		padding: 15px;
		margin: 0px;
		border-bottom: 1px solid rgba($colorTextNavbar, 0.3);
		.drawer__close {
			text-align: left;
			padding: 0px;
			@extend %text-24;
			@extend %text-bold;
			@extend %text-uppercase;
			color: $colorTextNavbar;
			button {
				float: right;
				right: 0;
				@extend %text-14;
				@extend %smooth-trans;
				@extend %text-uppercase;
				color: $colorTextNavbar;
				background: transparent;
				outline: 0px;
				border: 0px;
				border-radius: 0px;
				padding: 0px;
				svg {
					@extend %text-24;
				}
			}
		}
	}

	ul.mobile-nav {
		margin: 0px;
		> li {
			&.mobile-nav__search {
				padding: 15px !important;
				form {
					border-radius: 5px;
					overflow: hidden;
					input {
						height: 34px;
						line-height: 34px;
						padding: 0px 15px;
						border-radius: 0px;
						border: 0px;
						outline: 0px;
						width: 100%;
						background: #fff;
					}
					button {
						height: 34px;
						width: 34px;
						line-height: 34px;
						padding: 0px;
						border-radius: 0px;
						border: 0px;
						outline: 0px;
						background: #fff;
						color: $colorBgNavbar;
						@extend %text-18;
					}
				}
			}

			&.mobile-nav__item {
				padding: 0px;
				display: block;
				padding: 10px 15px;
				background: rgba($colorBgNavbar,0);
				@extend %smooth-trans;
				border-left: 3px solid transparent;
				border-bottom: 1px dashed rgba($colorTextNavbar, 0.3);

				> .mobile-nav__has-sublist {
					> a {
						width: auto !important;
						display: inline-block;
						position: relative;
						padding: 0px;
						@extend %text-bold;
						@extend %text-14;
						@extend %text-uppercase;
						@extend %smooth-trans;
						color: $colorTextNavbar;			
						&:after {
							content: '';
							display: block;
							border-bottom: 1px solid $colorTextNavbarHover;
							width: 0;
							position: absolute;
							left: 0;
							-webkit-transition: 1s ease;
							transition: 1s ease;
						}
						&:hover {
							opacity: 1 !important;
							color: $colorTextNavbarHover;
							&:after {
								width: 100%;
							}
						}
					}
					.mobile-nav__toggle button {
						padding: 0px;
						color: $colorTextNavbar;
						background: transparent;
						outline: 0px;
						border-radius: 0px;
						border: 0px;
						&:hover {
							color: $colorTextNavbarHover;
						}
					}
				}
				> .mobile-nav__sublist {
					padding-left: 15px;
					> .mobile-nav__item {
						padding: 10px 0px;
						&:last-child {
							padding-bottom: 0px;
						}
						> .mobile-nav__has-sublist {
							> a {
								display: inline-block;
								width: auto !important;
								position: relative;
								padding: 0px;
								@extend %text-14;
								@extend %smooth-trans;
								color: $colorTextNavbar;			
								&:after {
									content: '';
									display: block;
									border-bottom: 1px solid $colorTextNavbarHover;
									width: 0;
									position: absolute;
									left: 0;
									-webkit-transition: 1s ease;
									transition: 1s ease;
								}
								&:hover {
									opacity: 1 !important;
									color: $colorTextNavbarHover;
									&:after {
										width: 100%;
									}
								}
							}
							.mobile-nav__toggle button {
								padding: 0px;
								color: $colorTextNavbar;
								background: transparent;
								outline: 0px;
								border-radius: 0px;
								border: 0px;
								&:hover {
									color: $colorTextNavbarHover;
								}
							}
						}
						> .mobile-nav__sublist {
							> .mobile-nav__item {
								padding: 5px 0px 5px 15px;
								> a {
									display: inline-block;
									width: auto !important;
									position: relative;
									padding: 0px;
									@extend %text-14;
									@extend %smooth-trans;
									color: $colorTextNavbar;			
									&:after {
										content: '';
										display: block;
										border-bottom: 1px solid $colorTextNavbarHover;
										width: 0;
										position: absolute;
										left: 0;
										-webkit-transition: 1s ease;
										transition: 1s ease;
									}
									&:hover {
										opacity: 1 !important;
										color: $colorTextNavbarHover;
										&:after {
											width: 100%;
										}
									}
								}
							}
						}
					}
				}


				&:hover {
					background: rgba($colorBgNavbar,1);
					border-left: 3px solid $colorTextNavbarHover;
					> a {
						&:after {
							width: 100%;
						}
					}
				}
			}

			> a {
				display: inline-block;
				position: relative;
				padding: 0px;
				@extend %text-bold;
				@extend %text-14;
				@extend %text-uppercase;
				@extend %smooth-trans;
				color: $colorTextNavbar;			
				&:after {
					content: '';
					display: block;
					border-bottom: 1px solid $colorTextNavbarHover;
					width: 0;
					position: absolute;
					left: 0;
					-webkit-transition: 1s ease;
					transition: 1s ease;
				}
				&:hover {
					color: $colorTextNavbarHover;
					opacity: 1 !important;
				}
			}

			&:after {
				display: none;
			}
		}
	}
}
